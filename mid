//  write a c program print the prime number between given two inputs

#include <stdio.h>

int main()
{
    int i, j, a, prime;
    printf("Enter the range (two numbers): ");
    scanf("%d%d", &i, &a);

    if (i < 2) i = 2; 
    for (; i <= a; i++)
    {
        prime = 1;
        for (j = 2; j <= i / 2; j++)  
        {
            if (i % j == 0)
            {
                prime = 0;
                break;  
            }
        }
        if (prime == 1)
        {
            printf("%d ", i);
        }
    }
    printf("\n");
    return 0;
}

